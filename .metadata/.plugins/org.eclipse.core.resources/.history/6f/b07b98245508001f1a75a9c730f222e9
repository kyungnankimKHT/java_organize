package com.kh.fileEx;

import java.io.File;

public class FileEx {
	//ctrl shift f 줄 정렬
	public static void main(String[] args) {
		
		//파일만들고 글 작성하기
		String 파일이름 = "example.txt";
		
		File file = new File(파일이름);
		//파일이 존재하는지 확인
		if(file.exists()) {
			System.out.println("파일이 이미 존재합니다.");
		}
	}
}










