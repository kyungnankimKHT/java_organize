package com.kh.Sandwich;

public class 샌드위치 {
//필드
		private String 고객명;
		private String 샌드위치종류;

		
//메서드
	//생성자 필수 alt shift s o
		public 샌드위치(String 고객명, String 샌드위치종류) {
			super();
			this.고객명 = 고객명;
			this.샌드위치종류 = 샌드위치종류;
		}
	//Getter
	//Setter
		
	//@Override toString 고객명 샌드위치종류 alt shift s s
}
